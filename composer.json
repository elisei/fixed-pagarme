{
    "name": "o2ti/fixed-pagarme",
    "description": "O2TI - Corrige m√≥dulo da Pagarme que impede o uso com outros meios de pagamento",
    "require": {
        "php": "~7.4.0||~8.1.0||~8.2.0||~8.3.0",
        "pagbank/payment-magento": "*",
        "pagarme/pagarme-magento2-module": "*",
        "ext-json": "*"
    },
    "type": "magento2-module",
    "authors": [
        {
            "name": "Bruno Elisei",
            "email": "brunoelisei@o2ti.com"
        }
    ],
    "license": [
        "OSL-3.0",
        "AFL-3.0"
